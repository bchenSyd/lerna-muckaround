{
  "name": "lerna-test-bochen",
  "version": "0.68.0",
  "main": "index.js",
  "files": [
    "dummy.txt"
  ],
  "scripts": {
    "postinstall": "npm run lerna:bootstrap",
    "clean": "lerna clean --yes &&find packages -name 'dist' |xargs rm -rf ",
    "lerna:bootstrap": "lerna bootstrap --hoist --ci ",
    "//1": "can't use &>/dev/null because npm use /bin/sh, not bash",
    "publish-1": "lerna updated > /dev/null 2>&1; if [ $? -eq 0 ]; then  npm run lerna:publish ; else echo No changes to publish ; fi",
    "//": "end exit code is the second statement, not lerna udated. you have to use `;` , not `&` otherwise both get executed ",
    "publish-2": "lerna updated; if [ $? -eq 0 ]; then npm run lerna:publish; fi",
    "/////////////////////   master branch ///////////////////////////////////////": "",
    "release": "lerna publish --force-publish='*' --repo-version=1.0.3 --skip-git --registry http://localhost:5873",
    "prerelease": "npm run prepublish",
    "/////////////////////// develop branch //////////////////////////////////////": "",
    "publish": "lerna publish --registry http://localhost:4873",
    "prepublishOnly": "for p in packages/*; do cp .npmrc $p/; done",
    "////////////////////////////////////////////////////////////////////////////////////": "",
    "detect-changes": "lerna updated",
    "test": "cd packages/button && npm pack",
    "build:button": "rollup -c --gel-component=button",
    "build": "rollup -c --gel-component",
    "lint": "eslint packages",
    "gitSemverTags": "git-semver-tags",
    "debug-lerna": "node --inspect-brk node_modules/.bin/lerna publish --skip-git --skip-npm",
    "debug-bump": "lerna publish --skip-git --skip-npm --debug-bump"
  },
  "author": "",
  "license": "ISC",
  "description": "lerna test.",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "lerna": "3.0.0-beta.18",
    "lodash": "^4.17.5",
    "prettier": "^1.13.5",
    "react": "^16.2.0",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-url": "^1.3.0",
    "style-component": "^0.0.1",
    "yargs": "^11.0.0"
  },
  "dependencies": {}
}
