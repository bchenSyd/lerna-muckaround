{
  "lerna": "3.0.0-beta.18",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "conventionalCommits": true,
      "yes": true,
      "message": "chore(release): **NO_CI** [skip_ci]  publish %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "//": "https://github.com/lerna/lerna/issues/896#issuecomment-311894609",
  "registry": "https://registry.npmjs.org/",
  "version": "1.1.5"
}
