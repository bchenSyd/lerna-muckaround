{
  "lerna": "3.0.0-beta.17",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "conventionalCommits": true,
      "yes": true,
      "message": "chore(release): **NO_CI** [skip_ci]  publish %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "version": "1.0.8"
}
